<%- include('layouts/header') %>
<%- include('layouts/nav') %>

    <h1>This Is Contact Page</h1>
    <table class="table">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nama</th>
            <th scope="col">No Telp</th>
            <th scope="col">Email</th>
        </tr>
        </thead>
        <tbody>
            <% if (contact.length < 1) { %>
            <p>Data Contact Kosong</p>
            <% } else { %>
                <% contact.forEach((contacts, i) => { i++ %>
                    <tr>
                        <th scope="row"><%= i++ %></th>
                        <td><%= contacts.nama %></td>
                        <td><%= contacts.noTelp %></td>
                        <td><%= contacts.email %></td>
                    </tr>
                <% }) %>
            <% } %>
        </tbody>
    </table>

<%- include('layouts/footer') %>
